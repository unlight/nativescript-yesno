<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
  	<Button text="Yes or No?" tap="{{onTap}}"/>
    <WebView id="web-view" visibility="{{ isVisible ? 'visible' : 'collapse' }}" src="{{webViewSrc}}" />
    <Label text="{{answer}}" class="answer" />
  </StackLayout>
</Page>